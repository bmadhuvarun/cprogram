from collections import defaultdict
class Solution :
    def __init__(self,head_name):
        self.family=defaultdict(list)
        self.head=head_name
        self.dead=set()
    def birth(self,parent,child):
        self.family[parent].append(child)
    def death(self,name):
        self.dead.add(name)                                                                                                                                                                                                                                   
    def inhent(self):
        ans=[]
        def dfs(current):
            if current not in self.dead:
                ans.append(current)
                for child in self.family[current]:
                    dfs(child)
        dfs(self.head)
        return ans
ob=Solution("a")
ob.birth("a","c")
ob.birth("c","e")
ob.death("d")
result=ob.inhent()
print(result)
        
        
        
        
